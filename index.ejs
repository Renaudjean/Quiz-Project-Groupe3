<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ('page/templates/head')  %> 
    <title>QUIZZDOM | Main</title>
</head>
<body>
   <%- include('page/templates/header-hidden') %>
    <main>
        <div class="container">
            <div class="fond">
                <h1 class="h1-quiz">Choose your <span> Quizz </span></h1>
                <div class="flex-quiz">
                <% for(q of quiz) { %> 
                    <div class="quizz">
                        <img src="<%= q.Quiz_Photo %>" alt="Quiz <%= q.Quiz_Name %>" class="img-quiz">
                        <div class="quizz_content">
                            <h2 class="h2-quiz"><%= q.Quiz_Name %></h2>
                            <p class="p-quiz"><%= q.Quiz_Description%></p>
                            <a href="quiz/<%= q.Quiz_ID%>" class="btn-secondary">Choose quiz</a>
                    </div>
                </div> 
                <% } %> 
            </div>
        </div>
    </main>
</body>
</html>





